                        <Label fx:id="speedLabel" styleClass="label-bright" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <Label fx:id="effectivityLabel" styleClass="label-bright" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                        <Label text="Effectivity" GridPane.rowIndex="6" />
                     </children>
                  </GridPane>
                  <AnchorPane layoutX="136.0" layoutY="254.0">
                     <children>
                        <HBox>
                           <children>
                              <Button layoutX="201.0" layoutY="530.0" mnemonicParsing="false" onAction="#handleNewUnitArchetype" text="New..." />
                              <Button layoutX="253.0" layoutY="530.0" mnemonicParsing="false" onAction="#handleEditUnitArchetype" text="Edit..." />
                              <Button layoutX="305.0" layoutY="530.0" mnemonicParsing="false" onAction="#handleDeleteUnitArchetype" text="Delete" />
                           </children>
                        </HBox>
                     </children>
                  </AnchorPane>
               </children>
            </AnchorPane>
         </items>
      </SplitPane>
   </children>
